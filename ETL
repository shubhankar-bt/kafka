docker-compose file :

version: '3.8'
services:
  # 1. The Vector Database
  redis-vector:
    image: redis/redis-stack-server:latest
    container_name: redis-vector
    ports:
      - "6380:6379"
    volumes:
      - redis_data:/data

  # 2. The Local AI Engine
  ollama:
    image: ollama/ollama:latest
    container_name: ollama
    ports:
      - "11434:11434"
    volumes:
      - ./fincore-ai/local-models/models:/models

volumes:
  redis_data:
  ollama_data:
